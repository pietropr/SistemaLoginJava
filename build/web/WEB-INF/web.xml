<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cadastrauser</servlet-name>
        <servlet-class>servlet.Cadastrauser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consultaUsuario</servlet-name>
        <servlet-class>servlet.consultaUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultaUsuario</servlet-name>
        <servlet-class>servlet.ConsultaUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cadastracliente</servlet-name>
        <servlet-class>servlet.Cadastracliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cadastraproduto</servlet-name>
        <servlet-class>servlet.Cadastraproduto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultaCliente</servlet-name>
        <servlet-class>servlet.ConsultaCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Consultaproduto</servlet-name>
        <servlet-class>servlet.Consultaproduto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cadastrauser</servlet-name>
        <url-pattern>/Cadastrauser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consultaUsuario</servlet-name>
        <url-pattern>/consultaUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultaUsuario</servlet-name>
        <url-pattern>/ConsultaUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cadastracliente</servlet-name>
        <url-pattern>/Cadastracliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cadastraproduto</servlet-name>
        <url-pattern>/Cadastraproduto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultaCliente</servlet-name>
        <url-pattern>/ConsultaCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Consultaproduto</servlet-name>
        <url-pattern>/Consultaproduto</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
